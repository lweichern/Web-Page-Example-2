<!DOCTYPE html>
<html>
    <head>
        <title>Navbar 3</title>
        <link rel="stylesheet" href="../assets/stylesheet/styles.css">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
        <link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css" />
    </head>

    <body>
        <nav class="navbar-container">
            <ul class="navlinks-left" data-aos="zoom-in-down" data-aos-duration="1200">
                <li>
                    <a href="">Men</a>
                </li>
                <li>
                    <a href="">Women</a>
                </li>
                <li>
                    <a href="">Kids</a>
                </li>
                <li>
                    <a href="">Accesories</a>
                </li>
            </ul>

            <div class="navtitle" data-aos="flip-left" data-aos-duration="1200"> 
                NAVBAR
            </div>

            <ul class="navlinks-right" data-aos="zoom-in-up" data-aos-duration="1200">
                <li>
                    <a href="">Newsletter</a>
                </li>
                <li>
                    <a href="">Magazines</a>
                </li>
                <li>
                    <a href="">Others</a>
                </li>
                <li>
                    <a href="">Contact Us</a>
                </li>
            </ul>
        </nav>
        
        <!-- bootstrap carousel -->
        <div class="top-container">
            <div class="carousel-container" data-aos="zoom-out" data-aos-duration="1200" data-aos-delay="600">
                <div id="myCarousel" class="carousel slide" data-ride="carousel">
                   
                    <ol class="carousel-indicators">
                      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                      <li data-target="#myCarousel" data-slide-to="1"></li>
                      <li data-target="#myCarousel" data-slide-to="2"></li>
                      <li data-target="#myCarousel" data-slide-to="3"></li>
                    </ol>
                
                    <div class="carousel-inner" style="width: 100%; margin-left: auto; margin-right: auto; transition: 0.4s;">
                
                      <div class="item active men-fashion">
                        <img src="/assets/images/men-fashion.jpg" alt="Men Fashion" style="width:100%;height: 550px;transition: 0.4s;">
                        <div class="carousel-caption">
                            <h1 class="caption">Men Fashion</h1>
                            <a href="/teqball"><button>Check it out</button></a>
                        </div>
                      </div>
                
                      <div class="item carousel-item-2 women-fashion">
                        <img src="/assets/images/women-fashion.jpg" alt="Women Fashion" style="width:100%;height: 550px;transition: 0.4s;">
                        <div class="carousel-caption">
                            <h1 class="caption">Women Fashion</h1>
                            <a href="/teqball"><button>Check it out</button></a>
                        </div>
                      </div>
                      
                      <div class="item carousel-item-3 kids-fashion">
                        <img src="/assets/images/kids-fashion.jpg" alt="Kids Fashion" style="width:100%;height: 550px;transition: 0.4s;">
                        <div class="carousel-caption">
                            <h1 class="caption">Kids Fashion</h1>
                            <a href="/teqball"><button>Check it out</button></a>
                        </div>
                      </div>

                      <div class="item carousel-item-4 accessories">
                        <img src="/assets/images/accessories.jpg" alt="Accesories" style="width:100%;height: 550px;transition: 0.4s;">
                        <div class="carousel-caption">
                            <h1 class="caption">Accessories</h1>
                            <a href="/teqball"><button>Check it out</button></a>
                        </div>
                      </div>
                  
                    </div>
                
                    <!-- Left and right controls -->
                    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                      <span class="glyphicon glyphicon-chevron-left"></span>
                      <span class="sr-only">Previous</span>
                    </a>
                    <a class="right carousel-control" href="#myCarousel" data-slide="next">
                      <span class="glyphicon glyphicon-chevron-right"></span>
                      <span class="sr-only">Next</span>
                    </a>
                  </div>
            </div>
        </div>

        <!-- Spring section -->
        <div class="spring-section">
            <h1 data-aos="zoom-in" data-aos-duration="1200">Feel the Spring in us</h1>

            <div class="spring-content">
                <div class="image-container" data-aos="fade-right" data-aos-duration="1200">
                    <div class="picture img-1">
                        <img src="/assets/images/spring-1.jpg" alt="Spring Image 1">  
                    </div>
                    <div class="picture img-2">
                        <img src="/assets/images/spring-2.jpg" alt="Spring Image 2">
                    </div>
                    <div class="picture img-3">
                        <img src="/assets/images/spring-3.jpg" alt="Spring Image 3">
                    </div>
                    
                </div>
                <div class="description" data-aos="fade-left" data-aos-duration="1200">
                    <h2>Spring Season</h2>
                    <button>Check Out Our Collection</button>
                    <h5><< Click the photo for surprise</h5>
                </div>
            </div>
            
        </div>

    </body>
    <script src="../assets/scripts/script.js"></script>
    <script>
        // prevent slider auto slide
        $('.carousel').carousel({
            interval: false,
        });
    </script>
    <script src="https://unpkg.com/aos@next/dist/aos.js"></script>
    <script>
        AOS.init();
    </script>
</html>